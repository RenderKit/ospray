float sampleCylSolidAngleTable(float u, float v, float w)
{
  if (u > 1.f)
    u = 1.f;
  if (v > 1.f)
    v = 1.f;
  if (w > 1.f)
    w = 1.f;
  if (u <= 0.f || v <= 0.f || w <= 0.f)
    return 0.f;

  u *= 10.f;
  v *= 10.f;
  w *= 10.f;

  const int U0 = (int)u;
  const int V0 = (int)v;
  const int W0 = (int)w;

  const int U1 = U0 == 10 ? 10 : U0 + 1;
  const int V1 = V0 == 10 ? 10 : V0 + 1;
  const int W1 = W0 == 10 ? 10 : W0 + 1;

  u -= (float)U0;
  v -= (float)V0;
  w -= (float)W0;

  const float p00 = lerp(u,
      CylCapSolidAngleTable[121 * U0 + 11 * V0 + W0],
      CylCapSolidAngleTable[121 * U1 + 11 * V0 + W0]);
  const float p10 = lerp(u,
      CylCapSolidAngleTable[121 * U0 + 11 * V0 + W1],
      CylCapSolidAngleTable[121 * U1 + 11 * V0 + W1]);
  const float p01 = lerp(u,
      CylCapSolidAngleTable[121 * U0 + 11 * V1 + W0],
      CylCapSolidAngleTable[121 * U1 + 11 * V1 + W0]);
  const float p11 = lerp(u,
      CylCapSolidAngleTable[121 * U0 + 11 * V1 + W1],
      CylCapSolidAngleTable[121 * U1 + 11 * V1 + W1]);
  const float p0 = (1. - w) * p00 + w * p10;
  const float p1 = (1. - w) * p10 + w * p11;

  return (1. - v) * p0 + v * p1;
}